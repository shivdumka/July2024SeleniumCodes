<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="TestNG Regression suite" verbose="3" thread-count="3" parallel="tests">
	
	<test name="Open Cart Login Page Test">
		<parameter name="url" value="https://naveenautomationlabs.com/opencart/index.php?route=account/login"/>
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="testNGFeatures.LoginPageTest" />
		</classes>		
	</test>	
	
	<test name="Google">
		<parameter name="url" value="https://www.google.com"/>
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="testNGFeatures.GooglePageTest"/>
		</classes>
		
	</test>	
	
	<test name="OrangeHRM">
		<parameter name="url" value="https://orangehrm.com/30-day-free-trial"/>
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="testNGFeatures.OrangeHRMPageTest"></class>
		</classes>
		
	</test>	
</suite>